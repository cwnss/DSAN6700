{
  "id": null,
  "uid": "aws-metrics",
  "title": "AWS Metrics",
  "tags": ["aws", "s3"],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Total S3 Calls by Operation",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(aws_s3_calls_total) by (operation)",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Avg S3 Operation Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(aws_s3_operation_seconds_sum) by (operation) / sum(aws_s3_operation_seconds_count) by (operation)",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "Total S3 Uploads",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(aws_s3_calls_total{operation=\"upload\"}) or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "Avg Upload Time",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(aws_s3_operation_seconds_sum{operation=\"upload\"}) / sum(aws_s3_operation_seconds_count{operation=\"upload\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "Total Presigned URL Calls",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(aws_s3_calls_total{operation=\"get_presigned_url\"}) or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": []
      }
    },
    {
      "id": 6,
      "title": "Total S3 Calls",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(aws_s3_calls_total) or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": []
      }
    },
    {
      "id": 7,
      "title": "Total Data Uploaded",
      "type": "stat",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(aws_s3_bytes_total{direction=\"upload\"}) or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes" },
        "overrides": []
      }
    },
    {
      "id": 8,
      "title": "Data Uploaded Over Time",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "expr": "sum(aws_s3_bytes_total{direction=\"upload\"})",
          "legendFormat": "Total Bytes Uploaded",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes" },
        "overrides": []
      }
    }
  ]
}
